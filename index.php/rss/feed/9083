return array( 'pdfpreview' =&gt; array( &#13;
            'width' =&gt; array( 'type' =&gt; 'integer', 'required' =&gt; true, 'default' =&gt; 99 ),&#13;
            'height' =&gt; array( 'type' =&gt; 'integer', 'required' =&gt; true, 'default' =&gt; 0 ),&#13;
