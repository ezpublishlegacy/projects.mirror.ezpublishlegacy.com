    <item>
      <title>typo in paradoxpdf.php</title>
      <link>http://projects.ez.no/paradoxpdf/forum/issues/typo_in_paradoxpdf_php</link>
      <description>paradoxpdf.php, line 168&#13;
&#13;
$pdfConent = '';&#13;
